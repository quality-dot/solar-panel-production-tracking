{
  "name": "paperless",
  "version": "1.0.0",
  "description": "Paperless project with taskmaster-ai integration",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "backend": "node backend/server.js",
    "backend:dev": "nodemon backend/server.js",
    "backend:prod": "NODE_ENV=production node backend/server.js",
    "backend:health": "node backend/scripts/dev.js health",
    "backend:test-data": "node backend/scripts/dev.js test-data",
    "backend:load-test": "node backend/scripts/dev.js load-test",
    "db:migrate": "node database/scripts/migrate.js run",
    "db:migrate:status": "node database/scripts/migrate.js status",
    "db:migrate:rollback": "node database/scripts/migrate.js rollback",
    "db:migrate:create": "node database/scripts/create-migration.js",
    "db:init": "psql -h ${DB_HOST:-localhost} -p ${DB_PORT:-5432} -U postgres -f database/scripts/init-database.sql",
    "db:reset": "npm run db:init && npm run db:migrate",
    "logs": "tail -f backend/logs/combined.log",
    "logs:error": "tail -f backend/logs/error.log",
    "generate-barcodes": "node backend/scripts/generateBarcodes.js",
    "gen-test-data": "node backend/scripts/generateBarcodes.js test-dataset --output test-barcodes.json",
    "load-test": "node backend/scripts/loadTest.js run",
    "load-test-stress": "node backend/scripts/loadTest.js run --stations 16 --barcodes 50",
    "benchmark": "node backend/scripts/loadTest.js benchmark",
    "cache-test": "node backend/scripts/loadTest.js cache",
    "test-db-pool": "node backend/scripts/test-postgresql-pool.js",
    "test-api-routes": "node backend/scripts/test-api-routes.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "fastify": "^5.5.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "pg-pool": "^3.10.1",
    "socket.io": "^4.8.1",
    "task-master-ai": "^0.24.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "yup": "^1.7.0",
    "zod": "^3.25.76"
  }
}
