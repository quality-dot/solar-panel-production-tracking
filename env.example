# TaskMaster AI Configuration
# Copy this file to .env and fill in your API keys

# Required: Anthropic API Key (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: OpenAI API Key (GPT)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Google API Key (Gemini)
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Perplexity API Key
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Optional: Mistral API Key
MISTRAL_API_KEY=your_mistral_api_key_here

# Optional: Groq API Key
GROQ_API_KEY=your_groq_api_key_here

# Optional: OpenRouter API Key
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: XAI API Key
XAI_API_KEY=your_xai_api_key_here

# Optional: Azure OpenAI API Key
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here

# Optional: Ollama API Key (for local models)
OLLAMA_API_KEY=your_ollama_api_key_here

# TaskMaster AI Settings
TASKMASTER_LOG_LEVEL=info
TASKMASTER_DEBUG=false 