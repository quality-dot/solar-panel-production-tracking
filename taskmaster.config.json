{
  "project": {
    "name": "Paperless",
    "version": "1.0.0",
    "description": "Paperless project with taskmaster-ai integration"
  },
  "ai": {
    "defaultModel": "claude-3-sonnet-20240229",
    "maxTokens": 4000,
    "temperature": 0.7,
    "defaultSubtasks": 3,
    "defaultPriority": "medium"
  },
  "tasks": {
    "storeInGit": true,
    "autoExpand": false,
    "complexityThreshold": 6
  },
  "mcp": {
    "enabled": true,
    "server": "task-master-ai"
  }
} 